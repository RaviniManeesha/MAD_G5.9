<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    android:backgroundTintMode="add"
    android:textAlignment="center"
    tools:context=".MainActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="vertical">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textAlignment="center">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout"
                android:layout_width="409dp"
                android:layout_height="654dp"
                tools:layout_editor_absoluteX="1dp"
                tools:layout_editor_absoluteY="75dp">

                <RelativeLayout
                    android:id="@+id/relativeLayout14"
                    android:layout_width="363dp"
                    android:layout_height="142dp"
                    android:background="#4F4F53"
                    android:orientation="vertical"
                    android:textAlignment="center"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.403"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.08">

                    <TextView
                        android:id="@+id/textView24"
                        android:layout_width="314dp"
                        android:layout_height="58dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="40dp"
                        android:layout_marginTop="29dp"
                        android:layout_marginEnd="9dp"
                        android:layout_marginBottom="55dp"
                        android:fontFamily="@font/aclonica"
                        android:text="CA Marks"
                        android:textAlignment="center"
                        android:textColor="#070707"
                        android:textSize="24sp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="#1B171745">

                        <Button
                            android:id="@+id/button6"
                            android:layout_width="113dp"
                            android:layout_height="46dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="131dp"
                            android:layout_marginTop="73dp"
                            android:layout_marginEnd="118dp"
                            android:layout_marginBottom="23dp" />
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relativeLayout13"
                    android:layout_width="363dp"
                    android:layout_height="142dp"
                    android:background="#4F4F53"
                    android:orientation="vertical"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.456"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.435">

                    <TextView
                        android:id="@+id/textView26"
                        android:layout_width="301dp"
                        android:layout_height="59dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="40dp"
                        android:layout_marginTop="27dp"
                        android:layout_marginEnd="22dp"
                        android:layout_marginBottom="56dp"
                        android:fontFamily="@font/aclonica"
                        android:text="Final Exam Mark"
                        android:textAlignment="center"
                        android:textColor="#0C0C0C"
                        android:textSize="24sp" />

                    <Button
                        android:id="@+id/button8"
                        android:layout_width="118dp"
                        android:layout_height="55dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="135dp"
                        android:layout_marginTop="68dp"
                        android:layout_marginEnd="110dp"
                        android:layout_marginBottom="19dp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relativeLayout12"
                    android:layout_width="363dp"
                    android:layout_height="142dp"
                    android:background="#4F4F53"
                    android:orientation="vertical"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.456"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.781">

                    <TextView
                        android:id="@+id/textView27"
                        android:layout_width="287dp"
                        android:layout_height="67dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="39dp"
                        android:layout_marginTop="22dp"
                        android:layout_marginEnd="37dp"
                        android:layout_marginBottom="53dp"
                        android:fontFamily="@font/aclonica"
                        android:text="Total Mark / Grade"
                        android:textColor="#070707"
                        android:textSize="24sp" />

                    <Button
                        android:id="@+id/button9"
                        android:layout_width="122dp"
                        android:layout_height="54dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="131dp"
                        android:layout_marginTop="71dp"
                        android:layout_marginEnd="110dp"
                        android:layout_marginBottom="17dp" />
                </RelativeLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/textView23"
                android:layout_width="400dp"
                android:layout_height="56dp"
                android:fontFamily="@font/aclonica"
                android:text="Final Grade"
                android:textAlignment="center"
                android:textColor="#090909"
                android:textSize="36sp"
                app:layout_constraintBottom_toBottomOf="@+id/constraintLayout"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.454"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
</LinearLayout>